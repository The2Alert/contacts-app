{
  "name": "contacts-app",
  "description": "Contacts App.",
  "version": "1.0.0",
  "author": "dev2alert",
  "main": "./dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node .",
    "build": "rimraf ./dist && tsc",
    "dev": "cross-env NODE_ENV=development tsc-watch --onSuccess \"node .\"",
    "migration:generate": "typeorm migration:generate ./src/migrations/migration -t -d \"./dist/ormconfig.js\"",
    "migration:run": "typeorm migration:run -d \"./dist/ormconfig.js\"",
    "migration:show": "typeorm migration:show -d \"./dist/ormconfig.js\"",
    "migration:revert": "typeorm migration:revert -d \"./dist/ormconfig.js\"",
    "format": "prettier --write ./src",
    "frontend:build": "cross-env NODE_ENV=production webpack --config ./src/webpack.config.ts",
    "frontend:format": "prettier --write ./frontend/src"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.8",
    "@nestjs/core": "^9.0.8",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fs-extra": "^10.1.0",
    "mime": "^3.0.0",
    "module-alias": "^2.2.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "sha256": "^0.2.0",
    "sharp": "^0.30.7",
    "typeorm": "^0.3.7",
    "validator": "^13.7.0",
    "webpack": "^5.74.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/sha256": "^0.2.0",
    "@types/sharp": "^0.30.5",
    "@types/validator": "^13.7.5",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@types/whatwg-fetch": "^0.0.33",
    "css-loader": "^6.7.1",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
    "normalize.css": "^8.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4",
    "webpack-cli": "^4.10.0",
    "whatwg-fetch": "^3.6.2"
  },
  "_moduleAliases": {
    "@root": "./dist",
    "@config": "./dist/config"
  }
}
